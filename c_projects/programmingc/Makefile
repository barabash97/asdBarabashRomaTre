.PHONY: clean All

All:
	@echo "----------Building project:[ sum_of_fattororiale - Debug ]----------"
	@cd "sum_of_fattororiale" && "$(MAKE)" -f  "sum_of_fattororiale.mk"
clean:
	@echo "----------Cleaning project:[ sum_of_fattororiale - Debug ]----------"
	@cd "sum_of_fattororiale" && "$(MAKE)" -f  "sum_of_fattororiale.mk" clean
